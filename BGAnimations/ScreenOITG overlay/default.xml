		File="../_dark streak"
		InitCommand="horizalign,left;zoomy,.3;X,SCREEN_CENTER_X-120;y,SCREEN_CENTER_Y+30;ZoomToWidth,430;faderight,1"
		OnCommand="addx,-430;cropleft,1;sleep,.6;decelerate,.3;cropleft,0;addx,430;"
		/>
		
