		File="r"
		OnCommand="x,35;zoom,1.6;diffusealpha,0;sleep,.8;linear,.8;diffusealpha,1;sleep,.4;decelerate,.7;zoom,1;addx,-31;sleep,1;linear,.7;diffusealpha,0;"
